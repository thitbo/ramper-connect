{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    // "plugin:@nrwl/nx/react", "../../.eslintrc.json",
    "plugin:react/recommended"
    // "standard"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": "off"
  }
}
